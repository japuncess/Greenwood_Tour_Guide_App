<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorGreen"
        tools:context="com.robotemi.sdk.sample.EndLocationActivity">


    <RelativeLayout
        android:layout_width="1480dp"
        android:layout_height="600dp"
        android:orientation="vertical">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ImageViewEnd"
            android:src="@drawable/screen1"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="1280dp"
        android:layout_height="150dp"
        android:orientation="vertical"
        android:layout_marginTop="650dp"
        android:background="@color/colorGreen"
        >


        <Button
            android:id="@+id/btnEndTourSelectLocation"
            android:layout_width="345dp"
            android:layout_height="120dp"
            android:layout_marginLeft="450dp"
            android:padding="5dp"
            android:onClick="endTour"
            android:text="@string/end_tour"
            android:textColor="@color/colorWhite"
            android:textSize="28sp"
            android:background="@drawable/btn_round"
            android:textStyle="bold"
            />
    </RelativeLayout>

</RelativeLayout>
